{
  "name": "ag-awsauth",
  "description": "auth to aws sso/iamv2 easily",
  "main": "dist/index.js",
  "author": "andrei gec (andreigec@hotmail.com)",
  "license": "ISC",
  "private": false,
  "version": "0.0.150",
  "preferGlobal": true,
  "scripts": {
    "format": "eslint --ext .ts,.tsx ./src --fix",
    "lint": "tsc && eslint --ext .ts,.tsx ./src",
    "start": "tsc && node bin/awsauth.js",
    "build": "tsc"
  },
  "bin": {
    "ag-awsauth": "./bin/awsauth.js"
  },
  "files": [
    "bin/**/*",
    "dist/**/*",
    "README.md",
    "LICENSE.md"
  ],
  "dependencies": {
    "@aws-sdk/client-sso": "3.427.0",
    "@aws-sdk/client-sso-oidc": "3.427.0",
    "@aws-sdk/client-sts": "3.427.0",
    "@aws-sdk/shared-ini-file-loader": "3.370.0",
    "ag-common": "0.0.512",
    "cli-select": "1.1.2",
    "dotenv": "16.3.1",
    "envfile": "6.18.0",
    "eslint-config-e7npm": "0.0.43",
    "ini": "4.1.1",
    "node-fetch": "2.7.0",
    "puppeteer": "21.3.8",
    "readline-sync": "1.4.10",
    "typescript": "5.2.2",
    "yargs": "17.7.2"
  },
  "devDependencies": {
    "@types/ini": "1.3.31",
    "@types/node": "20.8.4",
    "@types/node-fetch": "2.6.6",
    "@types/readline-sync": "1.4.5",
    "@types/yargs": "17.0.28"
  },
  "engines": {
    "node": ">=18",
    "yarn": "use pnpm",
    "npm": "use pnpm",
    "pnpm": ">=8"
  }
}
