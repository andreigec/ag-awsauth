{
  "name": "ag-awsauth",
  "description": "auth to aws sso/iamv2 easily",
  "main": "dist/index.js",
  "author": "andrei gec (andreigec@hotmail.com)",
  "license": "ISC",
  "private": false,
  "version": "0.0.259",
  "preferGlobal": true,
  "scripts": {
    "format": "eslint --ext .ts,.tsx src --fix",
    "lint:tsc": "tsc --noEmit",
    "lint:eslint": "eslint --ext .ts,.tsx src",
    "lint": "pnpm run /^lint:/",
    "start": "tsc && node bin/awsauth.js",
    "build": "tsc"
  },
  "bin": {
    "ag-awsauth": "./bin/awsauth.js"
  },
  "files": [
    "bin/**/*",
    "dist/**/*",
    "README.md",
    "LICENSE.md"
  ],
  "dependencies": {
    "@aws-sdk/client-sso": "3.598.0",
    "@aws-sdk/client-sso-oidc": "3.600.0",
    "@aws-sdk/client-sts": "3.600.0",
    "@aws-sdk/shared-ini-file-loader": "^3.374.0",
    "ag-common": "0.0.719",
    "cli-select": "1.1.2",
    "dotenv": "16.4.5",
    "envfile": "7.1.0",
    "eslint-config-e7npm": "0.0.108",
    "ini": "4.1.3",
    "node-fetch": "2.7.0",
    "puppeteer": "22.12.0",
    "readline-sync": "1.4.10",
    "typescript": "5.5.2",
    "yargs": "17.7.2"
  },
  "devDependencies": {
    "@types/ini": "4.1.1",
    "@types/node": "20.14.8",
    "@types/node-fetch": "2.6.11",
    "@types/readline-sync": "1.4.8",
    "@types/yargs": "17.0.32"
  },
  "resolutions": {
    "ws": ">=8.17.1"
  },
  "engines": {
    "node": ">=18",
    "yarn": "use pnpm",
    "npm": "use pnpm",
    "pnpm": ">=8"
  }
}
