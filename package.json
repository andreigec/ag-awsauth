{
  "name": "ag-awsauth",
  "description": "auth to aws sso/iamv2 easily",
  "main": "dist/index.js",
  "author": "andrei gec (andreigec@hotmail.com)",
  "license": "ISC",
  "private": false,
  "version": "0.0.2",
  "preferGlobal": true,
  "scripts": {
    "format": "eslint --ext .ts,.tsx ./src --fix",
    "lint": "tsc && eslint --ext .ts,.tsx ./src",
    "start": "ts-node src/direct.ts",
    "build": "tsc"
  },
  "bin": {
    "ag-awsauth": "./bin/awsauth.js"
  },
  "files": [
    "bin/**/*",
    "dist/**/*",
    "README.md",
    "LICENSE.md"
  ],
  "dependencies": {
    "@aws-sdk/client-sso": "3.271.0",
    "@aws-sdk/client-sso-oidc": "3.271.0",
    "@aws-sdk/client-sts": "3.271.0",
    "@aws-sdk/shared-ini-file-loader": "3.271.0",
    "ag-common": "0.0.412",
    "cli-select": "1.1.2",
    "dotenv": "16.0.3",
    "envfile": "6.18.0",
    "esbuild": "0.17.8",
    "eslint-config-e7npm": "0.0.8",
    "ini": "3.0.1",
    "node-beep": "0.0.3",
    "node-fetch": "2.6.9",
    "puppeteer": "19.7.0",
    "readline-sync": "1.4.10",
    "ts-node": "10.9.1",
    "typescript": "4.9.5",
    "yargs": "17.6.2"
  },
  "devDependencies": {
    "@types/ini": "1.3.31",
    "@types/node": "18.13.0",
    "@types/node-fetch": "2.6.2",
    "@types/readline-sync": "1.4.4",
    "@types/yargs": "17.0.22"
  },
  "engines": {
    "node": ">=16.0.0",
    "yarn": "use pnpm",
    "npm": "use pnpm",
    "pnpm": ">=3"
  }
}